package Problem;

public class Problem_4 {
    private static int findFactorial(int n) {
        if (n == 0 || n == 1) {
            return 1;
        } else {
            return n * findFactorial(n - 1);
        }
    }

    public static int calculateFactorial(int n) {
        return findFactorial(n);
    }
}
