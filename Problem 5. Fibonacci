package Problem;

public class Problem_5 {
    private static int findFibonacci(int n) {
        if (n == 0 || n == 1) {
            return n;
        } else {
            return findFibonacci(n - 1) + findFibonacci(n - 2);
        }
    }

    public static int calculateFibonacci(int n) {
        return findFibonacci(n);
    }
}
